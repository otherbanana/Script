<script type="text/javascript">

Date.prototype.format = function (fmt) { //author: meizz 
    var o = {
        "M+": this.getMonth() + 1, //月份 
        "d+": this.getDate(), //日 
        "h+": this.getHours(), //小时 
        "m+": this.getMinutes(), //分 
        "s+": this.getSeconds(), //秒 
        "q+": Math.floor((this.getMonth() + 3) / 3), //季度 
        "S": this.getMilliseconds() //毫秒 
    };
    if (/(y+)/.test(fmt)) fmt = fmt.replace(RegExp.$1, (this.getFullYear() + "").substr(4 - RegExp.$1.length));
    for (var k in o)
    if (new RegExp("(" + k + ")").test(fmt)) fmt = fmt.replace(RegExp.$1, (RegExp.$1.length == 1) ? (o[k]) : (("00" + o[k]).substr(("" + o[k]).length)));
    return fmt;
}

function stringToDate (str) {
    if (!str || str.length != 19) return null;
    var pattern = /(\d{4})-(\d{2})-(\d{2}) (\d{2}):(\d{2}):(\d{2})/;
    var arrayDate = str.match(pattern);
    if (!arrayDate) return null;
    var isoStr = arrayDate[1] + '-' + arrayDate[2] + '-' + arrayDate[3] + 'T' + arrayDate[4] + ':' + arrayDate[5] + ':' + arrayDate[6] + 'Z';
    return getUTCDate(new Date(isoStr));
}

function getUTCDate(date) {
    return new Date(date.getUTCFullYear(), date.getUTCMonth(), date.getUTCDate(),  date.getUTCHours(), date.getUTCMinutes(), date.getUTCSeconds())
}

function httpGet(theUrl, func) {
    var xhr = function(){
        if (window.XMLHttpRequest) {
            return new XMLHttpRequest();
        }else{
            return new ActiveObject('Micrsorf.XMLHttp');
        }
    }();
    xhr.onreadystatechange = function(){
        switch(xhr.readyState) {
            case 4 :
                if ((xhr.status >= 200 && xhr.status < 300) || xhr.status == 304) {
                    func(xhr.responseText);
                } else {
                    alert('HTTP Error:' + xhr.status);
                }
                break;
        }
    }
    xhr.open('GET', theUrl);
    xhr.send(null);
}

function showCurrentDeviceIsActivated () {
    document.getElementById('div-current-device-is-activated').style.display = '';

    document.getElementById('btn-activate-current-device').style.display = 'none';

    var buttons = document.getElementsByClassName('btns-activate-current-device');
    for (var i=0; i<buttons.length; i++) {
        buttons[i].style.display = 'none';
    }
}

function copyText (text) {
    window.prompt('Select all and copy here:', text);
}
